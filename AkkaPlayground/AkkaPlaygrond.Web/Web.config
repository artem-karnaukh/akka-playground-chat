    <hocon>
        <![CDATA[
akka {
    loglevel = WARNING
    loggers = ["Akka.Logger.NLog.NLogLogger, Akka.Logger.NLog"]
    actor {
        provider = "Akka.Cluster.ClusterActorRefProvider, Akka.Cluster"
    }      
    remote {
        helios.tcp {
            hostname = "127.0.0.1"
            port = 56444
        }
    }
    cluster {
        seed-nodes = ["akka.tcp://AkkaCluster@127.0.0.1:2552"]
        roles = [nodeWorker]
        sharding {
            guardian-name = shardingWorkers
            role = "nodeWorker"   
            journal-plugin-id = "akka.persistence.shard-journal.sql-server"
            snapshot-plugin-id = "akka.persistence.shard-snapshot-store.sql-server"
            state-store-mode = "persistence"
        }
        singleton {
              singleton-name = "singleton"
              role = ""
              hand-over-retry-interval = 1s
              min-number-of-hand-over-retries = 10
        }
        coordinator-singleton  {
              singleton-name = "singleton"
              role = ""
              hand-over-retry-interval = 1s
              min-number-of-hand-over-retries = 10
        }
    }
            
    persistence {
        journal.plugin = "akka.persistence.journal.sql-server"
        snapshot-store.plugin = "akka.persistence.snapshot-store.sql-server"
        
        journal {
            sql-server {
                class = "Akka.Persistence.SqlServer.Journal.SqlServerJournal, Akka.Persistence.SqlServer" 
                connection-string = "Server=(local);initial catalog=AkkaPlayground;User=SourceWebUser; pwd=code-source-uk;"
                schema-name = dbo
                auto-initialize = on
            }
            shard-sql-server {
                class = "Akka.Persistence.SqlServer.Journal.SqlServerJournal, Akka.Persistence.SqlServer" 
                connection-string = "Server=(local);initial catalog=AkkaPlayground;User=SourceWebUser; pwd=code-source-uk;"
                schema-name = dbo
                auto-initialize = on
            }
        }
        snapshot-store {
            sql-server {
                class = "Akka.Persistence.SqlServer.Snapshot.SqlServerSnapshotStore, Akka.Persistence.SqlServer"
                connection-string = "Server=(local);initial catalog=AkkaPlayground;User=SourceWebUser; pwd=code-source-uk;"
                schema-name = dbo
                auto-initialize = on
            }
            shard-sql-server {
                class = "Akka.Persistence.SqlServer.Snapshot.SqlServerSnapshotStore, Akka.Persistence.SqlServer"
                connection-string = "Server=(local);initial catalog=AkkaPlayground;User=SourceWebUser; pwd=code-source-uk;"
                schema-name = dbo
                auto-initialize = on
            }
        }
        shard-journal {
            sql-server {
                  class = "Akka.Persistence.SqlServer.Journal.SqlServerJournal, Akka.Persistence.SqlServer" 
                  connection-string = "Server=(local);initial catalog=AkkaPlayground;User=SourceWebUser; pwd=code-source-uk;"
                  schema-name = dbo
                  auto-initialize = on
            }
        }
        shard-snapshot-store {
            sql-server {
                class = "Akka.Persistence.SqlServer.Snapshot.SqlServerSnapshotStore, Akka.Persistence.SqlServer"
                connection-string = "Server=(local);initial catalog=AkkaPlayground;User=SourceWebUser; pwd=code-source-uk;"
                schema-name = dbo
                auto-initialize = on
            }
        }
    }
}
